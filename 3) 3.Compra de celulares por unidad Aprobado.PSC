Proceso Ejercicio3
	
	
	
	Definir Precio Como Real;
	
	
	
	Definir Costo_A_Abonar Como Real;
	
	
	
	Precio <- 0;
	
	
	
	Costo_A_Abonar <- 0;
	
	
	
	Escribir 'Ingrese el precio por celular';
	
	
	
	Leer Precio;
	
	
	
	Costo_A_Abonar <- Precio*3;
	
	
	
	Escribir 'El costo a abonar es ',Costo_A_Abonar;
	
	
	
FinProceso
